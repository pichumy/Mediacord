.home {
  width: 100vw;
  height: 100vh;
  -webkit-box-direction: normal;
  -webkit-box-orient: vertical;
  background-color: #36393f;
  display: flex;
  position: absolute;
  overflow: hidden;
  flex: 1 1 auto;
  flex-wrap: nowrap;
  -webkit-box-pack: start;
  justify-content: flex-start;
  -webkit-box-align: stretch;
  align-items: stretch;
}
